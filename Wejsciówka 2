#include <iostream>
#include <vector>

using namespace std;

int main()
{
    cout << "Zajecia nr3\n";


    vector<int>tab;


    tab.push_back(10);


    int a = 8;
    tab.push_back(a);

    cout << tab[0] << endl;

    tab[1] = 6;
    cout << tab[1] << endl;

    for (
        int i = 0; 
        i < 3; 
        i++
        )
    {
        cout << tab[i] << endl;

    }
